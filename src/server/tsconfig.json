{
  "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "sourceMap": true,
      "outDir": "../../dist/server",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "paths": {
          "@shared/*": [
              "../shared/*"
          ]
      }
  },
  "exclude": [
      "**/*.test.ts"
  ]
}
